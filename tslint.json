{
  "extends": "tslint-config-airbnb",
  "linterOptions": {
    "exclude": [
      "node_modules/**/*",
      "out/**/*"
    ]
  },
  "rules": {
    "max-line-length": [
      true,
      200
    ],
    "import-name": false,
    "strictNullChecks": false,
    "no-boolean-literal-compare": true,
    "whitespace": false,
    "quotemark": [
      true,
      "single"
    ],
    "prefer-template": false
  }
}